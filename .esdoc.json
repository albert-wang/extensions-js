{
  "source": "./src",
  "includes": ["muxy.js", "sdk.js", "twitch-client.js", "state-client.js", "user.js", "util.js"],
  "destination": "./dist/docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "logo": "./assets/images/logo.png",
          "title": "Muxy Extensions SDK",
          "description": "The missing backend to Twitch Extensions",
          "repository": "https://github.com/muxy/extensions-js",
          "site": "https://dev.muxy.io",
          "author": "Muxy, Inc"
        },
        "unexportedIdentifier": {
          "enable": true
        }
      },
      "test": {
        "source": "./spec/",
        "interfaces": ["describe", "it", "context", "suite", "test"],
        "includes": ["(spec|Spec|test|Test)\\.js$"],
        "excludes": ["\\.config\\.js$"]
      }
    },
    {
      "name": "esdoc-publish-html-plugin",
      "option": { "template": "docs/template" }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
